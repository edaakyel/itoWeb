    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/strapi": "^0.3.1",
    "@types/ua-parser-js": "^0.7.36",
    "core-js": "2",
    "lodash": "^4.17.21",
    "nuxt": "^2.15.7",
    "nuxt-i18n": "^6.28.0",
    "nuxt-jsonld": "^1.5.1",
    "nuxt-property-decorator": "^2.9.1",
    "portal-vue": "^2.1.7",
    "store": "^2.0.12",
    "swiper": "^6.8.0",
